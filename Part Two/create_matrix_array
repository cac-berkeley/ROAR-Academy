import numpy as np

def set_array(L, rows, cols):
    try:
            return np.array(L).reshape(rows, cols)
    except ValueError as e:
        raise ValueError(
            f"Cannot reshape list of length {len(L)} into {rows} rows and {cols} columns"
        ) from e


L = [1, 2, 3, 4, 5, 6]
print(set_array(L, 2, 3))